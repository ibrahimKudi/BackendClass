<div class="author">
    <h1 class="author__heading">Search Results......</h1>
  </div>
  
  <section class="articles">    
  
    <ul class="article-ul">
      <% data.forEach(blog => { %>
        <li>
          <a href="/blog/<%= blog._id %>">
            <span><%= blog.title %></span>
            <span class="article-list__date"><%= blog.createdAt.toDateString() %></span>
          </a>
        </li>
        <% }) %>
    </ul>
  
  
      <a href="#" class="pagination">&lt; View Older Posts</a>
  
  
  </section>